<p id="notice"><%= notice %></p>

<h1>Listing Textos</h1>

<table>
  <thead>
    <tr>
      <th>Tex titulo</th>
      <th>Tex texto</th>
      <th>Post</th>
      <th>Tex imagen tmp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @textos.each do |texto| %>
      <tr>
        <td><%= texto.tex_titulo %></td>
        <td><%= texto.tex_texto %></td>
        <td><%= texto.post %></td>
        <td><%= texto.tex_imagen_tmp %></td>
        <td><%= link_to 'Show', texto %></td>
        <td><%= link_to 'Edit', edit_texto_path(texto) %></td>
        <td><%= link_to 'Destroy', texto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Texto', new_texto_path %>
